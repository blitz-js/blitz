{
  "name": "@blitzjs/cli",
  "description": "Blitz.js CLI",
  "version": "0.29.2",
  "license": "MIT",
  "scripts": {
    "b": "./bin/run",
    "clean": "rimraf lib",
    "predev": "yarn wait:installer && yarn wait:server && yarn wait:generator && yarn wait:repl",
    "wait:generator": "wait-on ../generator/dist/packages/generator/src/index.d.ts",
    "wait:server": "wait-on ../server/dist/packages/server/src/index.d.ts",
    "wait:installer": "wait-on ../installer/dist/packages/installer/src/index.d.ts",
    "wait:repl": "wait-on ../repl/dist/packages/repl/src/index.d.ts",
    "dev": "rimraf lib && tsc --watch --preserveWatchOutput",
    "build": "rimraf lib && tsc",
    "test": "tsdx test",
    "test:watch": "jest --watch"
  },
  "author": {
    "name": "Brandon Bayer",
    "email": "b@bayer.ws",
    "url": "https://twitter.com/flybayer"
  },
  "main": "lib/src/index.js",
  "bin": {
    "blitz": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "dependencies": {
    "@blitzjs/display": "0.29.2",
    "@blitzjs/repl": "0.29.2",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "3.2.1",
    "@oclif/plugin-not-found": "1.2.4",
    "@prisma/sdk": "2.14.0",
    "@salesforce/lazy-require": "0.3.3",
    "camelcase": "^6.2.0",
    "chalk": "4.1.0",
    "cross-spawn": "7.0.3",
    "dotenv-expand": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "enquirer": "2.3.6",
    "got": "^11.8.1",
    "has-yarn": "2.1.0",
    "hasbin": "1.2.3",
    "import-cwd": "3.0.0",
    "minimist": "1.2.5",
    "p-event": "4.2.0",
    "pkg-dir": "^5.0.0",
    "pluralize": "^8.0.0",
    "rimraf": "^3.0.2",
    "tar": "^6.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "3.9.0",
    "v8-compile-cache": "2.2.0"
  },
  "devDependencies": {
    "@blitzjs/generator": "0.29.2",
    "@blitzjs/installer": "0.29.2",
    "@blitzjs/server": "0.29.2",
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@prisma/cli": "2.14.0",
    "nock": "13.0.5",
    "stdout-stderr": "0.1.13"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "oclif": {
    "commands": "./lib/src/commands",
    "bin": "blitz",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "hooks": {
      "init": [
        "./lib/src/check-before-running"
      ]
    }
  },
  "keywords": [
    "blitz",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blitz-js/blitz"
  },
  "gitHead": "d3b9fce0bdd251c2b1890793b0aa1cd77c1c0922"
}
