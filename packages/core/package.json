  "module": "dist/core.esm.js",
  "types": "dist/packages/core/src/index.d.ts",
  "files": [
    "dist"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blitz-js/blitz"
  },
  "dependencies": {
    "@blitzjs/display": "0.13.0",
