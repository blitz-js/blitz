{
  "name": "blitz",
  "version": "0.0.0",
  "scripts": {
    "build": "tsup index-browser.tsx && tsup index-server.ts",
    "dev": "tsup index-browser.tsx --watch",
    "lint": "TIMING=1 eslint . --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "main": "./dist/index-server.js",
  "module": "./dist/index-server.mjs",
  "browser": "./dist/index-browser.js",
  "types": "./dist/index-server.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "react-query": "3.34.8"
  },
  "devDependencies": {
    "@blitzjs/config": "workspace:*",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
