{
  "extends": "@blitzjs/config/tsconfig.nextjs.json",
  "include": ["**/*.ts", "**/*.tsx", "next-env.d.ts", "types", ".next/types/**/*.ts"],
  "compilerOptions": {
    "paths": {
      "react": ["./node_modules/@types/react"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "exclude": ["node_modules"],
  "baseUrl": "."
}
